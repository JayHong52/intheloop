<div class="container">
    <h1 class="p-3">
        <%= title %> 
    </h1>
    <div class="row">
        <div class="col">
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">PostedBy</th>
                    <th scope="col">Date</th>
                    <th scope="col">Title</th>
                    <th scope="col"></th>
                    </th>
                  </tr>
                </thead>
                <tbody>
                    <% for (let count = 0; count < surveyList.length; count++) { %> 
                    <tr>
                        <th scope="row">
                            <%= count + 1 %> 
                        </th>
                        <td><%= surveyList[count].userName %> </td>
                        <td> Date </td>

                        <td><%= surveyList[count].title %> </td>
                        <td class="text-end"> 
                            <a href="/survey/edit/<%= surveyList[count]._id %>" class="btn btn-secondary btn-sm edit"><i class="fas fa-edit fa-sm"></i> TakeSurvey </a>
                            <a href="/survey/edit/<%= surveyList[count]._id %>" class="btn btn-dark btn-sm edit"><i class="fas fa-edit fa-sm"></i> ViewResult </a>
                        </td>
                    </tr>
                    <% } %> 
                </tbody>
              </table>
        </div>
    </div>
</div>